annel" data-url="https://eaccif.daouoffice.com/dmcif-doapi-service/channel/access/ess"> <img src="https://gnb.daouoffice.com/resources/images/channel_icon/channel_ac_adv_v2.png"></a>');    channelMenuEl.push('   </li>');    document.getElementById(append_el_id).insertAdjacentHTML('beforeend', channelMenuEl.join(""));    var channelArray = document.querySelectorAll('[data-name="channel"]');    for(var i = 0; i < channelArray.length; i++) {        var channelLiEl = channelArray[i].parentElement;        var code = channelLiEl.getAttribute('data-code');        if(templateData.preview || (templateData.channelCodes && templateData.channelCodes.indexOf(code) >= 0)){            channelLiEl.style.display = "block";        }        channelArray[i].addEventListener('click', function (event) {            removeForm('channelForm');            var selectChannel = event.currentTarget;            var url = selectChannel.getAttribute('data-url');            var uuid = getMiniGnbUUID();            var formEl = [];            formEl.push("<form action='" + url + "' target='"+uuid+"' method='post' id='channelForm'>");            formEl.push("   <input type='hidden' name='email' value='" + templateData.email + "'>");            formEl.push("/<form>");            var $form = formEl.join("");            document.getElementsByTagName('body')[0].insertAdjacentHTML('beforeend', $form);            window.open("about:blank", uuid, "");            document.getElementById('channelForm').submit();        });    }    var channelLiArray = document.querySelectorAll('li[data-clientId]');    for(var i=0; i < channelLiArray.length; i++) {        var channelEl = channelLiArray[i];        var name = channelEl.getAttribute('data-' + templateData.locale + 'name');        channelEl.firstElementChild.insertAdjacentHTML('beforeend', '<p class="mini_gnb_txt" title="'+name+'">' + name + '</p>');    }    var doItemEl = document.getElementById('do_menu');    doItemEl.parentElement.style.display = "block";    doIt