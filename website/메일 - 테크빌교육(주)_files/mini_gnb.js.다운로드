entHTML('afterbegin', initWrapper.join(""));}/* 템플릿 호출시 전달받은 데이터를 기반으로 do에 상세 정보 요청. 응답받은 정보로 템플릿 render.*   - data가 없는경우 미리보기로 간주. 임의의 정보와 링크연결하면 안되는 url은 구성하지 않음.*  */function render(data) {    if (data && Object.keys(data).length > 0) {        templateData.clientId = data.clientId;        templateData.serviceUrl = data.serviceUrl;        templateData.locale = data.locale.toLowerCase();        if(data.clientId) {            renderTemplateByData(data);        } else {            var script = document.createElement("script");            script.src = data.serviceUrl + "/api/minignb/data?callback=renderTemplateByData";            document.getElementsByTagName('head')[0].appendChild(script);        }    } else {        templateData = {            preview: true,            clientId: 'testclient',            email: 'daou@daou.co.kr',            name: '김다우',            locale: 'ko',            siteAdmin: true,            serviceUrl: 'http://www.daouoffice.com',            doIconUrl: 'https://gnb.daouoffice.com/resources/images/channel_icon/channel_do.png',            profileImageUrl: 'https://gnb.daouoffice.com/resources/images/photo_profile_small.jpg',            exploreChannelUrl: 'https://www.daouoffice.com/features.jsp',            myInfoUrl: '',            logoutUrl: '',            doCareUrl: '',            membershipUrl: '',            isSaas:true        };        renderTemplateByData();    }}function renderTemplateByData(jsonData) {    if (jsonData) {        templateData.email = jsonData.email;        templateData.name = jsonData.name;        templateData.locale = templateData.locale ? templateData.locale.toLowerCase() : jsonData.locale.toLowerCase();        templateData.siteAdmin = jsonData.siteAdmin;        templateData.isSaas = jsonData.saas ? true : false;        templateData.doIconUrl = 'https://gnb.daouoffice.com/resources/images/channel_icon/channel_do.png';        templateData.profileImageUrl = templateData.serviceUrl + jsonData.profileImageUrl;        templateData.exploreChannelUrl = 'https://www.daouoffice.com/features.jsp';        templateData.myInfoUrl = templateData.serviceUrl + "/app/my/profile";        templateData.logoutUrl = templateData.serviceUrl + "/oauth-logout?clientId=" + (templateData.clientId ? templateData.clientId : "") + "&email=" + templateData.email;        if(jsonData.docareInfo) {            templateData.doCareCustom = jsonData.docareInfo.custom;            templateData.doCareUrl = jsonData.docareInfo.url;            templateData.doCareToken = jsonData.docareInfo.token;        } else {            templateData.doCareUrl = templateData.serviceUrl + "/api/docare/meta";        }        if(jsonData.paymentInfo) {            templateData.membershipUrl = jsonData.paymentInfo.url;            templateData.membershipToken = jsonData.paymentInfo.token;        } else {            templateData.membershipUrl = templateData.serviceUrl + "/api/payment/meta";        }        if(jsonData.channelCodes && jsonData.channelCodes.length > 0) {            templateData.channelCodes = jsonData.channelCodes;        }    }    initLang();    bindMiniGnbWrapperEvent();    renderMenu('mini_gnb');    renderProfile('mini_gnb');}function initLang() {    switch (templateData.locale) {        case 'ko' :            lang = koLang;            break;        case 'en' :            lang = enLang;            break;        case 'jp' :        case 'ja' :            lang = jpLang;            break;        case 'zh_CN':        case 'zh_cn' :            lang = zhcnLang;            break;        case 'zh_TW' :        case 'zh_tw' :            lang = zhtwLang;            break;        case 'vi' :            lang = viLang;            break;        default :            lang = koLang;    }}/* * 통합메뉴 내 기본 이벤트 바인딩.(메뉴, 프로필 click, 하위메뉴 mouseevent 등) */function bindMiniGnbWrapperEvent() {    miniGNB = {        profileTimeout: null,        menuTimeout: null,        mouseLeaveMiniGnb: function () {            this.profileTimeout = window.setTimeout(this.hideProfileMoreLayer, 700);            this.menuTimeout = window.setTimeout(this.hideMenuLayer, 700);        },        mouseLeaveHideProfile: function () {            this.profileTimeout = window.setTimeout(this.hideProfileMoreLayer, 700);        },        mouseEnterProfile: function() {            clearTimeout(this.profileTimeout);        },        showProfileMoreLayer: function () {            document.getElementById('mini_gnb_profile_more').style.display = "block";            this.hideMenuLayer();        },        hideProfileMoreLayer: function () {            document.getElementById('mini_gnb_profile_more').style.display = "none";        },        mouseLeaveHideMenu: function () {            this.menuTimeout = window.setTimeout(this.hideMenuLayer, 700);        },        mouseEnterMenu: function() {            clearTimeout(this.menuTimeout);        },        showMenuLayer: function () {            document.getElementById('mini_gnb_menus').style.display = "block";            this.hideProfileMoreLayer();        },        hideMenuLayer: function () {            document.getElementById('mini_gnb_menus').style.display = "none";        }    };}/* 와플메뉴 영역 render.*   - 채널메뉴*   - 공통메뉴*   - 링크메뉴* */function renderMenu(append_el_id) {    var menuWrapper = [];    menuWrapper.push('  <div class="channel">');    menuWrapper.push('      <a title="'+ lang.채널 +'" id="mini_gnb_icon" onclick="miniGNB.showMenuLayer()" class="btn_channel"></a>');    menuWrapper.push('      <div class="notremoval gnb_layer gnb_channel_layer on" id="mini_gnb_menus" style="display: none" onmouseenter="miniGNB.mouseEnterMenu()" onmouseleave="miniGNB.mouseLeaveHideMenu()">');    menuWrapper.push('          <div class="mini_gnb_content">');    menuWrapper.push('              <ul class="channel_list" id="channel_menu_list"></ul>');    menuWrapper.push('              <ul class="channel_btn_list" id="common_menus_list"></ul>');    menuWrapper.push('              <ul class="channel_btn_list btn_txt" id="lin