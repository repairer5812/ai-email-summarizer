Event();    if(templateData.clientId) {        bindRedirectDocare();        bindRedirectMembership();    } else {        bindDocareEvent();        bindMembershipEvent();    }    function bindRedirectDocare() {        document.getElementById('goDoCare').addEventListener('click', function () {            if(templateData.doCareCustom) {                window.open(templateData.doCareUrl, "_blank");            } else {                removeForm('docareForm');                var uuid = getMiniGnbUUID();                var formEl = [];                formEl.push("<form action='" + templateData.doCareUrl + "' target='"+ uuid +"' method='post' id='docareForm'>");                formEl.push("   <input type='hidden' name='token' value='" + templateData.doCareToken + "'>");                formEl.push("/<form>");                var $form = formEl.join("");                document.getElementsByTagName('body')[0].insertAdjacentHTML('beforeend', $form);                window.open("about:blank", uuid, "");                document.getElementById('docareForm').submit();            }        });    }    function bindDocareEvent() {        document.getElementById('goDoCare').addEventListener('click', function () {            removeForm('docareForm');            var xhttp = new XMLHttpRequest();            xhttp.onreadystatechange = function () {                if (xhttp.readyState == 4 && xhttp.status == 200) {                    var data = JSON.parse(xhttp.responseText).data;                    var uuid = getMiniGnbUUID();                    var formEl = [];                    formEl.push("<form action='" + data.url + "/user/sso' target='"+ uuid +"' method='post' id='docareForm'>");                    formEl.push("   <input type='hidden' name='token' value='" + data.token + "'>");                    formEl.push("/<form>");                    var $form = formEl.join("");                    document.getElementsByTagName('body')[0].insertAdjacentHTML('beforeend', $form);                    window.open("about:blank", uuid, "");                    document.getElementById('docareForm').submit();                }            };            xhttp.open("GET", templateData.doCareUrl, true);            xhttp.setRequestHeader('content-type', 'application/json;charset=UTF-8');            xhttp.send();        });    }    function bindRedirectMembership() {        removeForm('customerPortalForm');        document.getElementById('goCustomerPortal').addEventListener('click', createCustomerPortalForm);    }    function bindMembershipEvent() {        removeForm('customerPortalForm');        document.getElementById('goCustomerPortal').addEventListener('click', createCustomerPortalFormWithRequest);    }    function createCustomerPortalForm(event) {        var uuid = getMiniGnbUUID();        var formEl = [];        templateData.membershipUrl = "https://my.daouoffice.com";        formEl.push("<form action='" + templateData.membershipUrl + "/sso?code=" + encodeURIComponent(templateData.membershipToken) + "' target='" + uuid + "' method='get' id='customerPortalForm'>");        formEl.push("   <input type='hidden' name='code' value='" + templateData.membershipToken + "'>");        formEl.push("/<form>");        var $form = formEl.join("");        document.getElementsByTagName('body')[0].insertAdjacentHTML('beforeend', $form);        document.getElementById('customerPortalForm').submit();    }    function createCustomerPortalFormWithRequest(event){        var xhttp = new XMLHttpRequest();        xhttp.onreadystatechange = function () {            if (xhttp.readyState == 4 && xhttp.status == 200) {                var data = JSON.parse(xhttp.responseText).data;                var uuid = getMiniGnbUUID();                var formEl = [];                data.url = "https://my.daouoffice.com";                formEl.push("<form action='" + data.url + "/sso?code=" + encodeURIComponent(data.token) + "' target='" + uuid + "' method='get' id='customerPortalForm'>");                formEl.push("   <input type='hidden' name='code' value='" + data.token + "'>");                formEl.push("/<form>");                var $form = formEl.join("");                document.getElementsByTagName('body')[0].insertAdjacentHTML('beforeend', $form);                document.getElementById('customerPortalForm').submit();            }        };        xhttp.open("GET", templateData.membershipUrl, true);        xhttp.setRequestHeader('content-type', 'application/json;charset=UTF-8');        xhttp.send();    }    function bindAddonServiceInfoEvent() {        document.getElementById('goAddonServiceInfo').addEventListener('click', function () {            window.open("http://go.daouoffice.co.kr/rpR09A", "_blank");        });    }    function bindAiEvent() {        document.getElementById('goAi').addEventListener('click', function () {            window.open('http://go.daouoffice.co.kr/gpt_daou', '_blank',                'width=750,height=1000');        });    }}function renderLinkMenus(append_el_id) {    var linkMenuEl = [];    linkMenuEl.push('   ');    document.getElementById(append_el_id).insertAdjacentHTML('beforeend', linkMenuEl.join(""));    var linkMenuLiArray = document.getElementById('link_menus_list').querySelectorAll('li[data-id]');    for(var i=0; i < linkMenuLiArray.length; i++){        var linkMenu = linkMenuLiArray[i];        var name = linkMenu.getAttribute('data-' + templateData.locale + 'name');        linkMenu.firstElementChild.insertAdjacentHTML('beforeend', '<span class="mini_gnb_txt">' + name + '</span>');    }    bindLinkMenuEvent();    function bindLinkMenuEvent() {        var linkMenuArray = document.querySelectorAll('[data-name="linkMenu"]');        for(var i=0; i < linkMenuArray.length; i++) {            linkMenuArray[i].addEventListener('click', function (event) {                var selectMenu = event.target.parentElement;                var url = selectMenu.getAttribute('data-url');                var target = selectMenu.getAttribute('data-target');                window.open(url, target);            });        }    }}function renderProfile(append_el_id) {    var profileWrapper = [];    profileWrapper.push('   <div class="mini_gnb_profile" id="mini_gnb_profile">');    profileWrapper.push('       <span class="mini_gnb_photo"><a onclick="miniGNB.showProfileMoreLayer()" id="profile"><img id="sessionThumbnail"></a></span>');    profileWrapper.push('       <div class="notremoval gnb_layer gnb_profile_layer on" id="mini_gnb_profile_more" style="display: none" onmouseenter="miniGNB.mouseEnterProfile()" onmouseleave="miniGNB.mouseLeaveHideProfile()">');    profileWrapper.push('           <div class="mini_gnb_content">');    profileWrapper.push('               <img id="photo_thumb" src="">');    profileWrapper.push('               <p class="mini_gnb_name" id="profile_name"></p>');    profileWrapper.push('               <p class="mini_gnb_txt" id="profile_email"></p>');    profileWrapper.push('               <div class="mini_gnb_btn_wrap">');    profileWrapper.push('                  <a class="mini_gnb_btn_minor_s" data-name="profileMenu"><span class="mini_gnb_ic"></span><span class="mini_gnb_txt"  id="myInfoPage"></span></a>');    profileWrapper.push('                  <a class="mini_gnb_btn_major_s" data-name="profileMenu"><span class="mini_gnb_ic"></span><span class="mini_gnb_txt"  id="logout"></span></a>');    profileWrapper.push('               </div>');    profileWrapper.push('           </div>');    profileWrapper.push('       </div>');    profileWrapper.push('   </div>');    document.getElementById(append_el_id).insertAdjacentHTML('beforeend', profileWrapper.join(""));    renderProfileImage();    renderProfileMore();    function renderProfileImage() {        var profilePhoto = document.getElementById('sessionThumbnail');        var profileDetailPhoto = document.getElementById('photo_thumb');        profilePhoto.setAttribute('src', templateData.profileImageUrl);        profilePhoto.setAttribute('title', templateData.name);        profileDetailPhoto.setAttribute('src', templateData.profileImageUrl);        profileDetailPhoto.setAttribute('title', templateData.name);        document.getElementById('profile_name').insertAdjacentText('beforeend', templateData.name);        document.getElementById('profile_email').insertAdjacentText('beforeend', templateData.email);    }    function renderProfileMore() {        var myInfoMenu = document.getElementById('myInfoPage');        myInfoMenu.insertAdjacentText('beforeend', lang.기본정보);        myInfoMenu.setAttribute('title', lang.기본정보);        myInfoMenu.parentElement.setAttribute('href', templateData.myInfoUrl);        var logoutMenu = document.getElementById('logout');        logoutMenu.insertAdjacentText('beforeend', lang.로그아웃);        logoutMenu.setAttribute('title', lang.로그아웃);        logoutMenu.parentElement.setAttribute('href', templateData.logoutUrl);    }}function removeForm(formId) {    var formEl = document.getElementById(formId);    if(formEl) {        formEl.parentNode.removeChild(formEl);    }}function getMiniGnbUUID() {    return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {        var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 3 | 8);        return v.toString(16);    });}